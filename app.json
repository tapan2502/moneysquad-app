{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.tapan25.bolt",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "usesCleartextTraffic": true,
      "manifest": {
        "queries": [
          {
            "intent": [
              {
                "action": "android.intent.action.DIAL",
                "data": {
                  "scheme": "tel"
                }
              }
            ]
          },
          {
            "intent": [
              {
                "action": "android.intent.action.VIEW",
                "data": {
                  "scheme": "tel"
                }
              }
            ]
          },
          {
            "intent": [
              {
                "action": "android.intent.action.SENDTO",
                "data": {
                  "scheme": "mailto"
                }
              }
            ]
          },
          {
            "intent": [
              {
                "action": "android.intent.action.VIEW",
                "data": {
                  "scheme": "mailto"
                }
              }
            ]
          },
          {
            "intent": [
              {
                "action": "android.intent.action.VIEW",
                "data": {
                  "scheme": "whatsapp"
                }
              }
            ]
          }
        ]
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "tel"
            },
            {
              "scheme": "mailto"
            }
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bab227a6-1f63-435f-aeb1-9647dcd643ea"
      }
    }
  }
}
